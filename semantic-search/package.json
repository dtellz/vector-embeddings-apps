        "@types/papaparse": "^5.3.7",
        "@types/uuid": "^9.0.1",
        "@types/yargs": "^17.0.24",
        "@typescript-eslint/eslint-plugin": "^5.59.7",
        "@typescript-eslint/parser": "^5.59.7",
        "eslint": "^8.41.0",
        "eslint-config-prettier": "^8.8.0",
        "esm": "^3.2.25",
        "nodemon": "^2.0.22",
        "npm-run-all": "^4.1.5",
        "prettier": "^2.8.8",
        "rimraf": "^4.1.2",
        "ts-node": "^10.9.1",
        "typescript": "^5.0.4"
    },
    "dependencies": {
        "@pinecone-database/pinecone": "^0.1.6",
        "@xenova/transformers": "^2.0.1",
